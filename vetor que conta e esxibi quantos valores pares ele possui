#include <stdio.h>
int main(void){
    float notas[15];
    float media;
    float soma;

    printf("informe os elementos do vetor:\n");
    for (int i = 0; i <15; i++){
     scanf("%f", &notas[i]);
    }
     for (int i = 0; i <15; i++) {
        soma+= notas[i];
        media=soma;
        printf("%d", media);
     }
    printf("\n") ;
    return 0;
}
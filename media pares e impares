#include <stdio.h>

int main(){

  int s1=0;
  int s2=0;
  float media1,media2;

  for(int i=1; i<=20;i++){


    if(i% 2==0){
      printf("Pares:%d\n", i);
   
    s1=s1+i;
      
    }
   
   
  }

  for(int j=1;j<=20;j++){

    if(j % 2==1){
      printf("Impares:%d\n", j);
       s2=s2+j;
    }
  
   
  }
  
  media1=s1/10;
  media2=s2/10;
  printf("Media pares:%2.f\n", media1);
  printf("Media imapares:%2.f\n", media2);


  return 0;
}
